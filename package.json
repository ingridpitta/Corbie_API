{
  "name": "corbie_api",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "./src/*"
    ]
  },
  "scripts": {
    "build": "npm-run-all clean transpile",
    "clean": "rm -rf dist-server",
    "dev": "npm-run-all build server",
    "dev-watch": "nodemon",
    "server": "node ./dist-server/server.js",
    "start": "npm-run-all build server",
    "transpile": "babel ./src --out-dir dist-server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ingridpitta/Corbie_API.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ingridpitta/Corbie_API/issues"
  },
  "homepage": "https://github.com/ingridpitta/Corbie_API#readme",
  "dependencies": {
    "bcrypt": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.15",
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/runtime": "^7.9.2",
    "npm-run-all": "^4.1.5",
    "@babel/plugin-transform-runtime": "^7.9.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "nodemon": "^2.0.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "ignore": [],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties"
      ]
    ]
  }
}
